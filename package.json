    "es6-promise": "^4.2.4",
    "express": "^4.16.2",
    "express-graphql": "^0.7.1",
    "graphql": "^14.1.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "2.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.3.2",
    "react-relay": "^2.0.0",
    "react-router-dom": "^4.2.2",
    "relay-runtime": "^2.0.0"
  }
}
